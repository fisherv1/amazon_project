$('#show_list_details').html('<%= escape_javascript(render(:partial => "list_update/edit_list_details")) %>');
$('#list_details').dataTable({
        "bLengthChange":false,
        "iDisplayLength":20,
        "bAutoWidth":false,
        "sDom":'lfrtpi',
        "aoColumns":[{
            'bSortable': false,
            'sWidth': "20%"
        }]
    });

$('#edit_list').dialog({
  modal: true,
  resizable: true,
  draggable: true,
  height: 700,
  width: 1300,
  buttons: {
    Ok: function() {
      $(this).dialog('close');
    }
  }
});

$('#edit_list').dialog('open');
