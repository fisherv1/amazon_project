<% if @phone.errors.empty? -%>
  $('#contacts').prepend('<%= escape_javascript(render(:partial => "contacts/contact", :locals => {:contact => @phone})) %>');
  $("#new_phone")[0].reset();
  $("#new_contact").toggle('blind');
  $('#<%= dom_id(@phone) %>').effect('slide', {direction:'right'});  
<% end -%>