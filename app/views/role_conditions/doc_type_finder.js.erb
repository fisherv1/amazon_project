

<% if @master_doc_types.blank? %>
  $("#doctype_id").html('');

    $("#assign_role_master_doc_description").html("");
    $("#assign_role_submit").attr('disabled',true);

<% else %>

  <%  result = String.new %>
  <% for mdmt in @master_doc_types %>
    <% result += "<option value='#{mdmt.id}'>#{mdmt.name}</option>" %>

  <%end%>
  $("#doctype_id").html('<%=escape_javascript("#{result}")%>');
  $("#doctype_id").change();


<% end -%>