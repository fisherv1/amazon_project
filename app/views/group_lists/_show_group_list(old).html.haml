%ul{:style => "list-style-type:none;padding-left:0px;"}
  - @group_lists.each do |i|
    -div_for(i, :style => "width:50%;float:left;") do
      .info_container
        %div#group_list_in
          = link_to image_tag('delete_icon.png'), group_list_path(i), :class => 'delete group_list_delete', :style => "float:right;",:error_message_field => "Group"
          = i.id
          \--
          = i.list_header.name
          
%br.clear