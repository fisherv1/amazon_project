<% unless flash[:error].nil? %>
 $("#flash_message").html("<%=escape_javascript(flash[:error])%>");
 $("#flash_message").dialog( {
 modal:true,
 position: 'top',
 buttons: {

 OK: function(){

 $(this).dialog('close');
 }
 }
 });

  $('#flash_message').dialog('open');
<% else %>






$('.show_list_container').html('<%=escape_javascript(render(:partial => '/group_lists/show_group_list', :object => @list_headers)) %>');
$('#new_group_list').toggle('blind');
 $('#add_new_list').css("display", "");
  $('#close_new_group_list').css("display", "none");

<%end%>