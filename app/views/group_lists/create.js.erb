<% unless flash[:error].nil? %>
$("#flash_message_text").html("<%= escape_javascript(flash[:error]) %>");
$('#flash_message').dialog( {
    modal: true,
    resizable: false,
    draggable: false,
    height: "auto",
    width: "auto",
    buttons: {
      Ok: function() {
        $(this).dialog('close');
      }
    }
  });
$('#flash_message').dialog('option', 'title', 'Error');
$('#flash_message').parent().find("a").css("display","none");
$("#flash_message").parent().css('background-color','#D1DDE6');
$("#flash_message").css('background-color','#D1DDE6');
$('#flash_message').dialog('open');
<% else %>

$('#existing_group_list_list').append('<%= escape_javascript(render(:partial => "/group_lists/new_group_list", :locals =>{:group_list => @group_list })) %>');
 $('#<%= dom_id(@group_list) %>').show('slide', {direction:'right'});
$('.add_form_close').click();
$('#group_list_show_group').html(" <%= escape_javascript(render(:partial => '/group_lists/header_group_list_group')) %>");
$('#group_list_group_'+ '<%=escape_javascript("#{@select_group_id}")%>').addClass("container_selected");

<%end%>
enable_form_after_submit_finish();