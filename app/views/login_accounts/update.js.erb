<% unless flash[:error].nil? %>
  $("#flash_message").html("<%= escape_javascript(flash[:error]) %>");
  $('#flash_message').dialog( {
  modal: true,

  buttons: {
  Ok: function() {
  $(this).dialog('close');
  }
  }
  });
  $('#flash_message').dialog('open');

<% else %>
    $("#flash_feedback").html("<%= escape_javascript(flash[:message]) %>");
    $('#flash_feedback').fadeIn("slow");
    $('body').click(function(){$('#flash_feedback').fadeOut("slow")})
    $('#show_user').html('<%= escape_javascript(render(:partial => '/user_management/show_user_list', :object => @login_accounts)) %>');
    $('#edit_user').html('');
    $(".add_user").show();
    $('#login_account_mode').attr('mode','show');

<%end%>
    enable_form_after_submit_finish();