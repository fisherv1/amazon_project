<%- if (@tag_type.tag_meta_type.status == true) %>
    $('#tag_type').html("<%= escape_javascript(render(:partial => '/tag_settings/tag_type')) %>");

    $('#tag_type2').html("<%= escape_javascript(render(:partial => '/tag_settings2/tag_type')) %>");
    <%- if(@tag_type.status == true)  %>
    $('#tag').html("<%= escape_javascript(render(:partial => '/tag_settings/tag')) %>");

    $('#tag2').html("<%= escape_javascript(render(:partial => '/tag_settings2/tag')) %>");
    <%- else %>
    $('#tag').html("");

    $('#tag2').html("");
    <%- end%>
<%- else %>
    $('#tag_type').html("");
    $('#tag').html("");

    $('#tag_type2').html("");
    $('#tag2').html("");
<%- end%>