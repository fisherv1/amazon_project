<%- if @entity_type == "person"%>
  <%- for @people in @entities%>
    <%#= render(:partial => "#{@render_url}") %>
<%= render :inline => "#{@pp}"%>
    <%if @people != @entities.last%>
      <div style="page-break-after: always;">&nbsp;</div>
    <%end%>
  <%end%>

<%else%>
  <%- for @organisations in @entities%>
    <%= render(:partial => "#{@render_url}") %>
    <%if @organisations != @entities.last%>
      <div style="page-break-after: always;">&nbsp;</div>
    <%end%>
  <%end%>
<%end%>