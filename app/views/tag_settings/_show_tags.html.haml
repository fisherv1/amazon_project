%ul
  - @tag.each do |i|
    %li
      .info_container
        %div
          = button_to_function 'Delete', :class => "delete delete_tag", :style => "float:right;", :tag_id => i.id,:error_message_field => "Tag"
          = button_to_function 'Edit', :class => "edit_tag", :style => "float:right;", :tag_id => i.id
          = i.name