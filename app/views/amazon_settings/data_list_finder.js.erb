<% unless @amazonsetting.blank? %>
  <% options = String.new %>
  <% options += "<option value='0'> New Record </option>" %>
  <% for setting in @amazonsetting %>
    <% options += "<option value='#{setting.id}'>#{setting.name}</option>" %>
  <% end %>
  $("#data_list_field").html('<%=escape_javascript("#{options}")%>');
  <% else %>
  $("#data_list_field").html('');
<% end %>
