<% unless flash[:error].nil? %>
 $("#flash_message_text").html("<%=escape_javascript(flash[:error])%>");
 $("#flash_message").dialog( {
 modal: true,
                resizable: false,
                draggable: true,
                height: 'auto',
                width: 'auto',
 buttons: {

 OK: function(){

 $(this).dialog('close');
 }
 }
 });
 $('#flash_message').dialog('option', 'title', 'Error');
  $('#flash_message').parent().find("a").css("display","none");
            $("#flash_message").parent().css('background-color','#D1DDE6');
            $("#flash_message").css('background-color','#D1DDE6');

  $('#flash_message').dialog('open');
<% else %>

$('#existing_user_list_list').append('<%= escape_javascript(render(:partial => "/user_lists/new_user_list", :locals =>{:user_list => @user_list })) %>');
 $('#<%= dom_id(@user_list) %>').effect('slide', {direction:'right'});
$('.add_form_close').click();
$('#user_list_show_group').html(" <%= escape_javascript(render(:partial => '/user_lists/header_user_list_user')) %>");
$('#user_list_user_'+ '<%=escape_javascript("#{@select_login_account_id}")%>').addClass("container_selected");

<%end%>
enable_form_after_submit_finish();