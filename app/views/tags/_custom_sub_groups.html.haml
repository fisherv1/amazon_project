%ul
  -for group_type in group_meta_type.tags do
    %li{:level => "2"}
      .container{:class => "toggle_multilevel_options", :level => "2"}
        %div.options
          = link_to image_tag('edit_icon.png'), edit_tag_type_path(group_type), :class => 'get'
          = link_to image_tag('delete_icon.png'), tag_type_path(group_type), :class => 'delete'

        = group_type.name
      %br.clear