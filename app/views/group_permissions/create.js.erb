<%  unless flash[:error].nil? %>
$('#flash_message').html("<%=escape_javascript(flash[:error])%>");
$('#flash_message').dialog({
modal:true,
position: 'top',
buttons :{

OK: function(){

$(this).dialog('close');
}
}
});

$('#flash_message').dialog('open');

<%else%>


$('.add_form_close').click();
$('#existing_group_permission_permission').html('<%= escape_javascript(render(:partial => "/group_permissions/existing_group_permission_permission")) %>');

<%end%>
