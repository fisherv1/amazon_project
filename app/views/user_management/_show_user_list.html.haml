%h1 Existing Users
%br
%ul{:style => "list-style-type:none;padding-left:0px;"}
  - @login_accounts.each do |i|
    %li{:style => "width:50%;float:left;cursor:pointer;cursor:hand;"}
      .show_users{:person_id => i.person_id, :class => (i.login_status?) ? "container_active" : "container_inactive", :id => "login_account_#{i.id}", :login_account_id => i.id}
        %div#user_list_in
          %div
            = i.person_id
            \-
            = i.user_name
          
