- fields_for :transaction_type_detail, @cheque_detail do |t|
  .form_line
    .field_15
      = t.label :bank_name, "<span class='asterisk'>*</span>Bank Name"
      = t.collection_select :bank_name, Bank.find(:all, :order => "full_name"), :id, :full_name, {}, {:class => 'compulsory_field'}
    .field_15
      = t.label :bsb, "<span class='asterisk'>*</span>Branch No."
      = t.text_field :bsb, :class => 'compulsory_field'
    .field_30
      = t.label :name_on_cheque, "<span class='asterisk'>*</span>Name on Cheque"
      = t.text_field :name_on_cheque, :class => 'compulsory_field'
    .field_15
      = t.label :cheque_number, "<span class='asterisk'>*</span>Cheque Number"
      = t.text_field :cheque_number, :class => 'compulsory_field'
    .field_20
      = t.label :date_on_cheque, "<span class='asterisk'>*</span>Date on Cheque"
      = t.text_field :date_on_cheque, :class => "datepick compulsory_field"
