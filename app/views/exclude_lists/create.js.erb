<% if @exclude_list.errors.empty? -%>
  $('#exclude_lists').html('<%= escape_javascript(render(:partial => "exclude_lists/exclude_list", :collection => @exclude_lists)) %>');

  <%- @list_header_ids.each do |i| %>
    $('#list_header_<%= escape_javascript("#{i}")%>').addClass("selected");
  <%- end %>
<% end -%>

<%- if !@flag %>
$("#flash_message").html("PrimaryList can't be added to exclude list");
$('#flash_message').dialog( {
    modal: true,
  resizable: false,
    draggable: true,
    position: 'centre',
    height: 'auto',
    width: 'auto',
    buttons: {
      Ok: function() {
        $(this).dialog('close');
      }
    }
  });

          $('#flash_message').parent().find("a").css("display","none");
            $("#flash_message").parent().css('background-color','#D1DDE6');
            $("#flash_message").css('background-color','#D1DDE6');
$('#flash_message').dialog('open');
<%- end %>
$("#compiler_options").val('');
enable_form_after_submit_finish();