if ( "<%= escape_javascript(@login_account.nil?.to_s) %>" == "false") {
  $("#password_reset_username").attr("disabled", true);
  $("#password_reset_email_address").attr("disabled", true);
  $("#password_reset_submit").hide();
  $("#password_captcha").hide();
}

$("#login_password_reset_security_questions").html("<%= escape_javascript(render(:partial => "login_password_reset_security_questions"))%>");
